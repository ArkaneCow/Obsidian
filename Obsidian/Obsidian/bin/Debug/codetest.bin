function updateRecentCookie(){
if(Cookie.isEnabled() && window.location.host.indexOf('dictionary')!=-1) {
var val=Cookie.getCookie("recsrch");
var checker=Cookie.getCookie("chk_del");
if(val){
if(checker == null){
Cookie.deleteCookie('recsrch','/','.reference.com');
var exp = new Date();
exp.setFullYear(exp.getFullYear());
exp.setMonth(exp.getMonth());
exp.setDate(exp.getDate()+1);
exp.setHours(0);
exp.setMinutes(0);
var domain= ".reference.com"
Cookie.setCookie("chk_del", "true", exp, "/", domain);
}
}
}
}
updateRecentCookie();
